{
  "name": "crocobot",
  "version": "0.1.0",
  "description": "Snappy automated testing",
  "scripts": {
    "test": "jest",
    "verbose": true
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "jest": "^24.0.0",
    "jest-cucumber": "^2.0.11",
    "jest-image-snapshot": "^2.8.2",
    "puppeteer": "^1.6.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "dotenv": "^5.0.0",
    "jest-cli": "^24.0.0",
    "jest-extended": "^0.11.1",
    "jest-report": "^0.1.11",
    "jest-stare": "^1.18.1",
    "npm": "^6.2.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/*.steps.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "reporters": {
    "default": "jest-stare"
  }
}
